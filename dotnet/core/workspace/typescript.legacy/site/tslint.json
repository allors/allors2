{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "interface-name": [false],
      "max-line-length": [
        true,
        {
          "limit": 240,
          "ignore-pattern": "^import |^export {(.*?)}"
        }
      ]
    },
    "rulesDirectory": []
}
