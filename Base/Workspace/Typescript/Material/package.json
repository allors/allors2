{
  "name": "material",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "node ./postinstall.js",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "lint-fix": "tslint --fix --project ./src/tsconfig.app.json",
    "tc:test": "ng test --watch=false",
    "build-stats": "ng build --aot --optimization --stats-json",
    "stats": "webpack-bundle-analyzer dist/material/stats.json",
    "autotest": "npx tsc --project ./autotest/tsconfig.autotest.json && node ./out-tsc/autotest/autotest/main  && npx rimraf ./out-tsc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.0.0",
    "@angular/cdk": "~7.3.6",
    "@angular/common": "~8.0.0",
    "@angular/compiler": "~8.0.0",
    "@angular/core": "~8.0.0",
    "@angular/forms": "~8.0.0",
    "@angular/material": "^7.3.6",
    "@angular/material-moment-adapter": "^7.3.6",
    "@angular/platform-browser": "~8.0.0",
    "@angular/platform-browser-dynamic": "~8.0.0",
    "@angular/router": "~8.0.0",
    "bootstrap": "^4.3.1",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "moment": "^2.24.0",
    "rxjs": "~6.5.2",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular/cli": "~8.0.0",
    "@angular/compiler-cli": "~8.0.0",
    "@angular/language-service": "~8.0.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-teamcity-reporter": "^1.1.0",
    "lnk": "^1.1.0",
    "protractor": "~5.4.0",
    "rimraf": "^2.6.3",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.4.5",
    "typescript-tslint-plugin": "^0.3.1",
    "webpack-bundle-analyzer": "^3.3.2"
  }
