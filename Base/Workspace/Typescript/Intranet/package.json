{
  "name": "intranet",
  "version": "1.0.0",
  "scripts": {
    "postinstall": "node ./postinstall.js",
    "ng": "ng",
    "ng-high-memory": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "lint-fix": "tslint --fix --project ./src/tsconfig.app.json",
    "tc:test": "ng test --watch=false",
    "build-stats": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --prod --stats-json",
    "stats": "webpack-bundle-analyzer dist/intranet/stats.json",
    "autotest": "npx -p typescript tsc --project ./autotest/tsconfig.autotest.json && node ./out-tsc/autotest/autotest/main && npx rimraf ./out-tsc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.0",
    "@angular/cdk": "~9.0.0",
    "@angular/common": "~9.0.0",
    "@angular/compiler": "~9.0.0",
    "@angular/core": "~9.0.0",
    "@angular/forms": "~9.0.0",
    "@angular/material": "^9.0.0",
    "@angular/material-moment-adapter": "^9.0.0",
    "@angular/platform-browser": "~9.0.0",
    "@angular/platform-browser-dynamic": "~9.0.0",
    "@angular/router": "~9.0.0",
    "angular-calendar": "^0.28.2",
    "bootstrap": "^4.4.1",
    "date-fns": "^2.8.1",
    "jsnlog": "^2.29.0",
    "marked": "^0.8.0",
    "moment": "^2.24.0",
    "quill": "^1.3.7",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "turndown": "^5.0.3",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.1",
    "@angular/cli": "~9.0.1",
    "@angular/compiler-cli": "~9.0.0",
    "@angular/language-service": "~9.0.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@types/quill": "^1.3.10",
    "@types/turndown": "^5.0.0",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "karma-trx-reporter": "^0.4.3",
    "lnk": "^1.1.0",
    "protractor": "~5.4.0",
    "rimraf": "^3.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.7.5",
    "webpack-bundle-analyzer": "^3.4.1"
  }
}
